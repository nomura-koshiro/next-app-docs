{
  "name": "plain-next",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.20.0",
    "pnpm": "10.18.1"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run format:check && pnpm run lint && pnpm run stylelint && pnpm run typecheck",
    "check:fix": "pnpm run format && pnpm run lint:fix && pnpm run stylelint:fix && pnpm run typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "e2e": "playwright test",
    "ci": "pnpm run check && pnpm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "plop",
    "generate:feature": "plop feature",
    "generate:route": "plop route",
    "generate:component": "plop component",
    "generate:app-page": "plop app-page"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
